# -*- mode: snippet -*-
# name: buildPythonPackage
# key: buildPythonPackage
# --
{ stdenv
, buildPythonPackage
, fetchPypi
}:

let pname = "pname";
    version = "1.0.0";
in buildPythonPackage rec {
  src = fetchPypi {
    inherit pname version;
    sha256 = "126yxzxq7yqhkd3hxqksxrd5kv10iviq15d4lk464j4xi9cdbf23";
  };
  buildInputs = [ ];
  propagatedBuildInputs = [ ];
  meta = ${1:meta};
}